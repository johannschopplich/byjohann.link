{
  "name": "byjohann-link",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.29.1",
  "scripts": {
    "dev": "pnpm run uno:clean && nuxt dev",
    "build": "pnpm run uno:build && nuxt build",
    "generate": "pnpm run uno:build && nuxt generate",
    "preview": "nuxt preview",
    "uno:clean": "echo \"/**/\" > app/assets/uno.css",
    "uno:build": "unocss --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc -b --noEmit",
    "prepare": "pnpm run uno:clean && nuxt prepare"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.3.0",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/simple-icons": "^1.2.70",
    "@nuxtjs/color-mode": "^4.0.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@types/node": "^24.10.12",
    "@unocss/eslint-config": "^66.6.0",
    "@unocss/nuxt": "^66.6.0",
    "@vueuse/nuxt": "^14.2.0",
    "culori": "^4.0.2",
    "eslint": "^10.0.0",
    "nuxt": "^4.3.1",
    "nuxt-kirby": "^3.0.3",
    "postcss-nesting": "^14.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
