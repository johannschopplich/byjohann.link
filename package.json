    "test:types": "vue-tsc --noEmit",
    "prepare": "nuxi prepare"
  },
  "devDependencies": {
    "@byjohann/eslint-config": "^0.3.0",
    "@byjohann/ui": "^0.5.5",
    "@iconify-json/carbon": "^1.1.21",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/node": "^20.8.10",
    "@unlazy/nuxt": "^0.10.1",
    "@unocss/eslint-config": "^0.57.2",
    "@unocss/nuxt": "^0.57.2",
    "@vueuse/nuxt": "^10.5.0",
    "eslint": "^8.53.0",
    "nuxt": "^3.8.1",
    "nuxt-kql": "^1.0.4",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.22"
  },
  "pnpm": {
    "overrides": {
      "has": "npm:@nolyfill/has@latest"
    }
  }
