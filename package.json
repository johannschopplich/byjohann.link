{
  "name": "byjohann-link",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "dev": "pnpm run uno:clean && nuxi dev",
    "build": "pnpm run uno:build && nuxi build",
    "generate": "pnpm run uno:build && nuxi generate",
    "preview": "nuxi preview",
    "uno:clean": "echo \"/**/\" > assets/css/uno.css",
    "uno:build": "unocss \"**/*.vue\" --out-file assets/css/uno.css --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "prepare": "pnpm run uno:clean && nuxi prepare"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.1",
    "@byjohann/ui": "^0.6.6",
    "@iconify-json/carbon": "^1.1.34",
    "@iconify-json/logos": "^1.1.42",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@nuxt/fonts": "^0.7.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/node": "^20.12.13",
    "@unocss/eslint-config": "^0.60.3",
    "@unocss/nuxt": "^0.60.3",
    "@vueuse/nuxt": "^10.10.0",
    "eslint": "^9.3.0",
    "nuxt": "^3.11.2",
    "nuxt-kql": "^1.4.17",
    "nuxt-lcp-speedup": "^0.8.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.19"
  }
}
