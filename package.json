{
  "name": "byjohann-link",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "pnpm run uno:clean && nuxi dev",
    "build": "pnpm run uno:build && nuxi build",
    "generate": "pnpm run uno:build && nuxi generate",
    "preview": "nuxi preview",
    "uno:clean": "echo \"/**/\" > app/assets/uno.css",
    "uno:build": "unocss \"**/*.vue\" --out-file app/assets/uno.css --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "prepare": "pnpm run uno:clean && nuxi prepare"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@byjohann/ui": "^0.7.2",
    "@iconify-json/carbon": "^1.1.36",
    "@iconify-json/logos": "^1.1.43",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@nuxtjs/fontaine": "^0.4.3",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/node": "^20.14.10",
    "@unocss/eslint-config": "^0.61.3",
    "@unocss/nuxt": "^0.61.3",
    "@vueuse/nuxt": "^10.11.0",
    "eslint": "^9.6.0",
    "nuxt": "^3.12.3",
    "nuxt-kql": "^1.4.18",
    "nuxt-vitalizer": "^0.10.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "vue-tsc": "^2.0.26"
  }
}
